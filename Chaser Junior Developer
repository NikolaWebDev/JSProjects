let arrRank = [
    { name: 'NikolaG', rankNum: 300 },
    { name: 'NikolaDev', rankNum: 101 },
    { name: 'NikolaGDev', rankNum: 102 }
];

 arrRank.sort((a, b) => (a.rankNum > b.rankNum) ? 1 : -1);
console.log(arrRank);
 

average = (name, rankNum) => {
let sum = 0;
    for(let i = 0; i < arrRank.length; i++){
        sum += arrRank[i].rankNum;
    }
    return sum / arrRank.length;
};
console.log(average().toFixed(2)); 
